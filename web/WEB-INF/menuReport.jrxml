	<queryString>
		<![CDATA[Select  *
	From Menu m join MenuDetail md on m.MenuID = md.MenuID 
	join Recipe r on r.RecipeID = md.RecipeID
	where m.username = $P{username}
	order by m.Weekdays asc, 
	case
		when m.[Time] = 'B' then '1'
		when m.[Time] = 'L' then '2'
		when m.[Time] = 'D' then '3'
	end
	
]]>
	</queryString>
